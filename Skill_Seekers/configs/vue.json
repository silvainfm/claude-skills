  "categories": {
    "getting_started": ["quick-start", "introduction", "essentials"],
    "components": ["component", "props", "events"],
    "reactivity": ["reactivity", "reactive", "ref", "computed"],
    "composition_api": ["composition", "setup"],
    "api": ["api", "reference"]
  },
  "rate_limit": 0.5,
  "max_pages": 200
}
